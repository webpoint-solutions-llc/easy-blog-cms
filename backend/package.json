    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "nest-winston": "^1.8.0",
    "nestjs-command": "^3.1.2",
    "nestjs-i18n": "^10.2.6",
